{
  "name": "kusk",
  "displayName": "kusk",
  "description": "kusk from kubeshop.io",
  "version": "0.0.1",
  "publisher": "kubeshop",
  "bugs": {
    "url": "https://github.com/kubeshop/kusk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kubeshop/kusk.git"
  },
  "homepage": "https://github.com/kubeshop/kusk/vscode/blob/main/README.md",
  "license": "MIT",
  "preview": true,
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:kusk.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kusk.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.67.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4",
    "@vscode/test-electron": "^2.1.3"
  }
}
